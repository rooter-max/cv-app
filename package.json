{
  "name": "cv-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve",
    "build:dev": "ng build",
    "build": "NODE_ENV=production ng build --configuration production && ng run cv-app:server:production",
    "compose": "npm run build && docker-compose -f docker-compose.yml up -d --build",
    "start": "NODE_ENV=production node dist/server/main.js",
    "dev:ssr": "cross-env NODE_OPTIONS=--max_old_space_size=8192 ng run cv-app:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr:dev": "ng build && ng run cv-app:server",
    "prerender": "ng run cv-app:prerender",
    "prerender:prod": "ng run cv-app:prerender",
    "test": "ng test",
    "lint": "ng lint",
    "lint:eslint": "eslint . --ext .ts",
    "lint:eslint:fix": "ng lint --fix",
    "lint:sass": "stylelint --custom-syntax postcss-sass \"src/**/*.sass\"",
    "lint:commitlint": "npx commitlint -- --from HEAD~1 --to HEAD --verbose",
    "e2e": "ng .tmp/e2e",
    "postinstall": "ngcc",
    "release:patch": "git add -A && standard-version --release-as patch",
    "release:minor": "git add -A && standard-version --release-as minor",
    "release:major": "git add -A && standard-version --release-as major",
    "release-rc": "git add -A && standard-version -a --prerelease next-$(git rev-parse --abbrev-ref HEAD | grep -Eo '[[:digit:]]+')",
    "docker:build": "docker build -p 80:80",
    "prettier": "prettier --write \"./**/*.{ts,scss,html}\"",
    "pretty": "pretty-quick --check",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@datorama/akita": "^6.1.3",
    "@ngneat/until-destroy": "^8.0.4",
    "@nguniversal/express-engine": "^12.1.0",
    "@sentry/angular": "^6.10.0",
    "@sentry/node": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "angular-svg-icon": "^12.0.0",
    "express": "^4.15.2",
    "ngx-cookie": "^5.0.2",
    "ngx-cookie-backend": "^5.0.2",
    "pm2": "^5.0.4",
    "rxjs": "~6.6.0",
    "source-map-explorer": "^2.5.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular-eslint/builder": "~12.1.0",
    "@angular-eslint/eslint-plugin": "~12.1.0",
    "@angular-eslint/eslint-plugin-template": "~12.1.0",
    "@angular-eslint/schematics": "~12.1.0",
    "@angular-eslint/template-parser": "~12.1.0",
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.0",
    "@babel/core": "^7.13.16",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@compodoc/compodoc": "^1.0.9",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@nguniversal/builders": "^12.1.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/angular": "^6.1.21",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.1.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^11.0.0",
    "postcss-sass": "^0.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "tailwindcss": "^2.2.7",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  },
  "engines": {
    "node": "16.x"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "hooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  }
}
