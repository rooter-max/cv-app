{
  "name": "cv-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:eslint": "eslint . --ext .ts",
    "lint:fix": "ng lint --fix",
    "e2e": "ng .tmp/e2e",
    "dev:ssr": "cross-env NODE_OPTIONS=--max_old_space_size=8192 ng run cv-app:serve-ssr",
    "serve:ssr": "node dist/cv-app/server/main.js",
    "build:ssr:dev": "ng build && ng run cv-app:server",
    "build:ssr": "NODE_ENV=production ng build --configuration production && ng run cv-app:server:production",
    "prerender": "ng run cv-app:prerender",
    "prerender:prod": "ng run cv-app:prerender",
    "postinstall": "ngcc",
    "release:patch": "git add -A && standard-version --release-as patch",
    "release:minor": "git add -A && standard-version --release-as minor",
    "release:major": "git add -A && standard-version --release-as major",
    "release-rc": "git add -A && standard-version -a --prerelease next-$(git rev-parse --abbrev-ref HEAD | grep -Eo '[[:digit:]]+')",
    "docker:build": "docker build --rm -t \"cv-app/front:$npm_package_version\" .",
    "prettier": "prettier --write \"./**/*.{ts,scss,html}\"",
    "pretty": "pretty-quick --check",
    "lint-scss": "stylelint --custom-syntax postcss-sass \"src/**/*.sass\"",
    "lint-scss1": "stylelint --formatter verbose --fix \"{apps,libs}/**/*.sass\"",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "dependencies": {
    "@angular/animations": "~12.1.3",
    "@angular/common": "~12.1.3",
    "@angular/compiler": "~12.1.3",
    "@angular/core": "~12.1.3",
    "@angular/forms": "~12.1.3",
    "@angular/platform-browser": "~12.1.3",
    "@angular/platform-browser-dynamic": "~12.1.3",
    "@angular/platform-server": "~12.1.3",
    "@angular/router": "~12.1.3",
    "@datorama/akita": "^6.1.3",
    "@ngneat/until-destroy": "^8.0.4",
    "@nguniversal/express-engine": "^12.0.1",
    "@sentry/angular": "^6.11.0",
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "angular-svg-icon": "^12.0.0",
    "express": "^4.17.1",
    "ngx-cookie": "^5.0.2",
    "ngx-cookie-backend": "^5.0.2",
    "pm2": "^5.0.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1201.3",
    "@angular-eslint/builder": "~12.1.0",
    "@angular-eslint/eslint-plugin": "~12.1.0",
    "@angular-eslint/eslint-plugin-template": "~12.1.0",
    "@angular-eslint/schematics": "~12.1.0",
    "@angular-eslint/template-parser": "~12.1.0",
    "@angular/cli": "~12.1.3",
    "@angular/compiler-cli": "~12.1.3",
    "@babel/core": "^7.13.16",
    "@compodoc/compodoc": "^1.0.9",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@nguniversal/builders": "^12.0.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/angular": "^6.1.21",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.1.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^11.0.0",
    "postcss-sass": "^0.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "tailwindcss": "^2.2.7",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "hooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  }
}
